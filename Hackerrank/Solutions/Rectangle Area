#include <iostream>

using namespace std;
class Rectangle {
protected:
    // Make members protected so the derived class can access them
    int width;
    int height;

public:
    // Default constructor to initialize
    Rectangle() : width(0), height(0) {}

    // Display method for the base class
    void display() {
        cout << width << " " << height << endl;
    }
};

class RectangleArea : public Rectangle {
public:
    // Method to read data into the protected base class members
    void read_input() {
        cin >> width >> height;
    }

    // Overloaded display method for the derived class
    void display() {
        // Calculates and prints the area
        cout << (width * height) << endl;
    }
};

int main()
{
    /*
     * Declare a RectangleArea object
     */
    RectangleArea r_area;
    
    /*
     * Read the width and height
     */
    r_area.read_input();
    
    /*
     * Print the width and height
     */
    r_area.Rectangle::display();
    
    /*
     * Print the area
     */
    r_area.display();
    
    return 0;
}
